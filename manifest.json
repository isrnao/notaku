{
    "manifest_version": 3,
    "name": "NotionTaku",
    "description": "WebページをNotionに魚拓する拡張機能",
    "version": "1.0.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://api.notion.com/*"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
